	<changeSet author="mmuschol" id="create-hibernate-sequence" dbms="postgresql" context="production">
		<preConditions onFail="MARK_RAN">
			<and>
				<dbms type="postgresql"/>
				<!--It appears to be a known problem that sequenceExists does not work with Postgres. Using SQL check instead. -->
				<sqlCheck expectedResult="0">SELECT count(c.relname) FROM pg_class c WHERE c.relkind = 'S' and c.relname = 'hibernate_sequence'</sqlCheck>
			</and>
		</preConditions>
		<comment>A hibernate_sequence is required for postgresql and possibly others, e.g. Oracle.</comment>

		<createSequence sequenceName="hibernate_sequence" cacheSize="1" incrementBy="1" minValue="1"
		                maxValue="9223372036854775807" startValue="1" cycle="false"/>
	</changeSet>

	<changeSet author="mmuschol" id="drop-redundant-sequences" dbms="postgresql" context="production">
		<preConditions onFail="MARK_RAN">
			<and>
				<dbms type="postgresql"/>
				<!--It appears to be a known problem that sequenceExists does not work with Postgres. Using SQL check instead. -->
				<sqlCheck expectedResult="1">SELECT count(c.relname) FROM pg_class c WHERE c.relkind = 'S' and c.relname = 'application_id_seq'</sqlCheck>
			</and>
		</preConditions>
		<!-- Hibernate uses a single sequence, and we can remove all of these sequences created by Liquibase. -->
		<addDefaultValue tableName="Application" columnName="id" defaultValueSequenceNext="hibernate_sequence"/>
		<addDefaultValue tableName="ApplicationPlugin" columnName="id" defaultValueSequenceNext="hibernate_sequence"/>
		<addDefaultValue tableName="Comment" columnName="id" defaultValueSequenceNext="hibernate_sequence"/>
		<addDefaultValue tableName="CVUser" columnName="id" defaultValueSequenceNext="hibernate_sequence"/>
		<addDefaultValue tableName="Process" columnName="id" defaultValueSequenceNext="hibernate_sequence"/>
		<addDefaultValue tableName="PullRequest" columnName="id" defaultValueSequenceNext="hibernate_sequence"/>
		<addDefaultValue tableName="Repository" columnName="id" defaultValueSequenceNext="hibernate_sequence"/>
		<addDefaultValue tableName="SpectrumGroup" columnName="id" defaultValueSequenceNext="hibernate_sequence"/>
		<addDefaultValue tableName="SshHostKey" columnName="id" defaultValueSequenceNext="hibernate_sequence"/>
		<addDefaultValue tableName="SshKey" columnName="id" defaultValueSequenceNext="hibernate_sequence"/>
		<addDefaultValue tableName="SupportedApp" columnName="id" defaultValueSequenceNext="hibernate_sequence"/>
		<addDefaultValue tableName="SupportedPlugin" columnName="id" defaultValueSequenceNext="hibernate_sequence"/>
		<addDefaultValue tableName="UserDirectory" columnName="id" defaultValueSequenceNext="hibernate_sequence"/>

		<dropSequence sequenceName="application_id_seq"/>
		<dropSequence sequenceName="comment_id_seq"/>
		<dropSequence sequenceName="cvuser_id_seq"/>
		<dropSequence sequenceName="process_id_seq"/>
		<dropSequence sequenceName="pullrequest_id_seq"/>
		<dropSequence sequenceName="repository_id_seq"/>
		<dropSequence sequenceName="spectrumgroup_id_seq"/>
		<dropSequence sequenceName="sshhostkey_id_seq"/>
		<dropSequence sequenceName="sshkey_id_seq"/>
		<dropSequence sequenceName="supportedapp_id_seq"/>
		<dropSequence sequenceName="userdirectory_id_seq"/>
		<!-- If we entered this changeset, then this means that the 1.4 schema was created by liquibase. For Postgres databases and LONGBLOB columns, Liquibase
		     disagrees with Hibernate on the type that should be used. This needs to be fixed here to use the native Postgres type expected by Hibernate. -->
		<dropColumn tableName="Process" columnName="file"/>
		<addColumn tableName="Process">
			<column name="file" type="oid"/>
		</addColumn>
	</changeSet>

